{
  "network": {
    "allowedDomains": [
      "github.com",
        "*.github.com",
        "api.github.com",
        "*.npmjs.org"
    ],
    "deniedDomains": [],
    "httpProxyPort": 8888,
    "allowUnixSockets": ["/var/run/docker.sock"],
    "allowLocalBinding": false
  },
  "filesystem": {
    "denyRead": [
      "~/.srt-settings.json",
      "~/dotfiles/.srt-settings.json",
      "~/.ssh",
      "~/.aws",
      "~/.gnupg/",
      ".env",
      ".env.*",
      ".env*.local",
      "*.env",
      "~/.config/gcloud/",
      "*-credentials.json",
      "*serviceAccount*.json",
      "*service-account*.json",
      "~/.azure/",
      "~/.kube/",
      "kubeconfig",
      "*-secret.yaml",
      "secrets.yaml",
      "~/.docker/",
      "*.pem",
      "*.key",
      "*.p12",
      "*.pfx",
      "*.tfstate",
      "*.tfstate.backup",
      ".terraform/",
      ".vercel/",
      ".netlify/",
      "firebase-adminsdk*.json",
      "serviceAccountKey.json",
      ".supabase/",
      "~/.netrc",
      "~/.npmrc",
      "~/.pypirc",
      "~/.git-credentials",
      ".git-credentials",
      "dump.sql",
      "backup.sql",
      "*.dump"
    ],
    "allowWrite": [".", "src/", "test/", "/tmp"],
    "denyWrite": [
      "~/.srt-settings.json",
      "~/dotfiles/.srt-settings.json",
      "~/.ssh",
      "~/.aws",
      "~/.gnupg/",
      ".env",
      ".env.*",
      ".env*.local",
      "*.env",
      "~/.config/gcloud/",
      "*-credentials.json",
      "*serviceAccount*.json",
      "*service-account*.json",
      "~/.azure/",
      "~/.kube/",
      "kubeconfig",
      "*-secret.yaml",
      "secrets.yaml",
      "~/.docker/",
      "*.pem",
      "*.key",
      "*.p12",
      "*.pfx",
      "*.tfstate",
      "*.tfstate.backup",
      ".terraform/",
      ".vercel/",
      ".netlify/",
      "firebase-adminsdk*.json",
      "serviceAccountKey.json",
      ".supabase/",
      "~/.netrc",
      "~/.npmrc",
      "~/.pypirc",
      "~/.git-credentials",
      ".git-credentials",
      "dump.sql",
      "backup.sql",
      "*.dump",
      "/etc/",
      "/usr/",
      "/bin/",
      "/sbin/",
      "/boot/",
      "/root/",
      "~/.bash_history",
      "~/.zsh_history",
      "~/.node_repl_history",
      "~/.bashrc",
      "~/.zshrc",
      "~/.profile",
      "~/.bash_profile"
    ]
  },
  "ignoreViolations": {
    "npm": ["/private/tmp"]
  },
  "enableWeakerNestedSandbox": false,
  "enableWeakerNetworkIsolation": false
}
