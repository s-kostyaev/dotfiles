{
  "rules": {
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": 1,
    "react/jsx-no-undef": 1,
    "no-use-before-define": [
      2,
      "nofunc"
    ],
    "no-unused-vars": [
      1
    ],
    "no-shadow": [
      0
    ],
    "no-multi-spaces": [
      0
    ],
    "new-cap": [
      0
    ],
    "quotes": [
      0
    ],
    "key-spacing": [
      0
    ],
    "consistent-return": [
      0
    ]
  },
  "ecmaFeatures": {
    "jsx": true,
    "templateStrings": true,
    "spread": true,
    "modules": true,
    "generators": true,
    "forOf": true,
    "destructuring": true,
    "defaultParams": true,
    "classes": true,
    "blockBindings": true,
    "arrowFunctions": true
  },
  "env": {
    "node": true,
    "es6": true,
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint"
}
